#alias l = "ls -a"
#alias ls = "ls -a"
alias l="ll -alh"
alias v="vim"
alias dragon="conda activate dragons"
alias elist="conda env list"

alias tailf="tail -f"
alias jlremote="jupyter-lab --no-browser --port=1378"

alias tr="tree -L 1"
alias watchme="watch -n 10 \"qstat \""
alias tmux="tmux -f $HOME/.dotfiles/tmux.conf"
alias tls="tmux ls"
alias ta="tmux attach -t"
alias ml="module list"
alias use="du -sh ./"
alias num="lfs find . |wc -l"
alias cl="clear"
alias _meraxes_modules="module purge; module load gcc/11.1.0 openmpi/4.0.2 gsl/2.6  hdf5/1.10.5p   fftw3/3.3.8   cmake/3.18.2; module unload imkl/2019.5.281"
alias _gadget4_modules="module purge; module load python3-as-python openmpi/4.0.1 hdf5/1.10.5 fftw3/3.3.8 gsl/2.6" 
